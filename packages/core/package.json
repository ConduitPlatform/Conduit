{
  "name": "@conduit/core",
  "version": "1.0.0",
  "main": "dist/bin/www.js",
  "types": "dist/bin/www.d.ts",
  "scripts": {
    "start": "node dist/bin/www.js",
    "lint": "./node_modules/.bin/eslint .",
    "test": "mocha -r ts-node/register --timeout 10000 --exit --reporter nyan src/test/**",
    "test-with-coverage": "nyc --reporter=text mocha -r ts-node/register --timeout 10000 --exit src/test/**",
    "build": "rimraf dist && tsc",
    "postbuild": "copyfiles -u 1 src/*.proto ./dist/",
    "prepare": "npm run snyk-protect && npm run build",
    "prepublish": "npm run build",
    "snyk-protect": "snyk protect"
  },
  "license": "ISC",
  "dependencies": {
    "@conduit/utilities": "1.0.0",
    "@conduit/grpc-sdk": "1.0.0",
    "@conduit/config": "1.0.0",
    "@conduit/admin": "1.0.0",
    "@conduit/database-provider": "1.0.0",
    "@conduit/router": "1.0.0",
    "@conduit/sdk": "1.0.0",
    "@conduit/security": "1.0.0",
    "@grpc/proto-loader": "^0.5.4",
    "grpc": "^1.24.2",
    "appmetrics-dash": "^5.3.0",
    "convict": "^6.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-winston": "^4.0.2",
    "lodash": "^4.17.15",
    "winston": "^3.2.1",
    "snyk": "^1.321.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-http": "^4.2.0",
    "@types/convict": "^4.2.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/express": "~4.16.1",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "chai": "^4.2.0",
    "chai-http": "*",
    "mocha": "^7.1.1",
    "nodemon": "^1.12.1",
    "nyc": "^15.0.0",
    "copyfiles": "^2.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "snyk": true
}
