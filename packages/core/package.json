{
  "name": "@conduitplatform/core",
  "version": "1.0.1",
  "main": "dist/bin/www.js",
  "types": "dist/bin/www.d.ts",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "node dist/bin/www.js",
    "lint": "./node_modules/.bin/eslint .",
    "test": "mocha -r ts-node/register --timeout 10000 --exit --reporter nyan src/test/**",
    "test-with-coverage": "nyc --reporter=text mocha -r ts-node/register --timeout 10000 --exit src/test/**",
    "build": "npm run generateTypes && rimraf dist && tsc",
    "postbuild": "copyfiles -u 1 src/*.proto ./dist/",
    "prepare": "npm run build",
    "prepublish": "npm run build",
    "generateTypes": "sh build.sh"
  },
  "license": "ISC",
  "dependencies": {
    "@conduitplatform/admin": "^1.0.0",
    "@conduitplatform/commons": "^1.0.0",
    "@conduitplatform/config": "^1.0.0",
    "@conduitplatform/grpc-sdk": "^1.0.1",
    "@conduitplatform/router": "^1.0.0",
    "@conduitplatform/security": "^1.0.0",
    "@grpc/grpc-js": "^1.5.2",
    "@grpc/proto-loader": "^0.5.4",
    "convict": "^6.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-winston": "^4.0.2",
    "lodash": "^4.17.15",
    "winston": "^3.2.1",
    "ts-proto": "^1.112.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-http": "^4.2.0",
    "@types/convict": "^4.2.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/express": "~4.16.1",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "chai": "^4.2.0",
    "chai-http": "*",
    "copyfiles": "^2.2.0",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.8.2",
    "typescript": "~4.2.0"
  }
}
