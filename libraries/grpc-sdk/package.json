  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.esm.js"
    }
  },
  "engines": {
    "node": ">=14"
  },
  "type": "module",
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "npm run protoc",
    "build": "rimraf dist && tsup",
    "protoc": "sh build.sh"
  },
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.14.3",
    "@grpc/proto-loader": "^0.8.0",
    "@sesamecare-oss/redlock": "^1.2.0",
    "abort-controller-x": "^0.5.0",
    "convict": "^6.2.4",
    "express": "^5.2.1",
    "fast-jwt": "^6.1.0",
    "fs-extra": "^11.3.3",
    "ioredis": "^5.9.2",
    "lodash": "^4.17.23",
    "lodash-es": "^4.17.23",
    "nice-grpc": "^2.1.14",
    "nice-grpc-client-middleware-retry": "^3.1.13",
    "prom-client": "^15.1.3",
    "protobufjs": "^8.0.0",
    "winston": "^3.19.0",
    "winston-loki": "^6.0.8"
  },
  "directories": {
    "lib": "dist",
    "src": "src"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/ConduitPlatform/conduit.git",
    "directory": "libraries/grpc-sdk"
  },
  "devDependencies": {
    "@types/convict": "^6.1.6",
    "@types/express": "^5.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/google-protobuf": "^3.15.12",
    "@types/lodash": "^4.17.23",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "24.9.1",
    "copyfiles": "^2.4.1",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.11.1",
    "tslib": "^2.6.3",
    "tsup": "^8.5.1",
    "typescript": "~5.9.3"
  },
  "peerDependencies": {
    "@grpc/grpc-js": "^1.13.3"
  }
