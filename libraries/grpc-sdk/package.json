{
  "name": "@conduitplatform/grpc-sdk",
  "version": "1.1.3",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.esm.js"
    }
  },
  "engines": {
    "node": ">=14"
  },
  "type": "module",
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "npm run protoc",
    "build": "rimraf dist && rollup -c",
    "postbuild": "copyfiles -u 1 src/*.proto src/**/*.json ./dist/",
    "protoc": "sh build.sh"
  },
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.13.3",
    "@grpc/proto-loader": "^0.8.0",
    "@sesamecare-oss/redlock": "^1.2.0",
    "abort-controller-x": "^0.5.0",
    "convict": "^6.2.4",
    "express": "^5.1.0",
    "fast-jwt": "^6.0.2",
    "fs-extra": "^11.2.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "nice-grpc": "^2.1.12",
    "nice-grpc-client-middleware-retry": "^3",
    "prom-client": "^15.1.3",
    "protobufjs": "^7.2.6",
    "winston": "^3.12.0",
    "winston-loki": "^6.0.8"
  },
  "directories": {
    "lib": "dist",
    "src": "src"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/ConduitPlatform/conduit.git",
    "directory": "libraries/grpc-sdk"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/convict": "^6.1.6",
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/google-protobuf": "^3.15.12",
    "@types/lodash": "^4.17.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "24.9.1",
    "copyfiles": "^2.4.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.22.4",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.8.0",
    "tslib": "^2.6.3",
    "typescript": "~5.9.3"
  },
  "peerDependencies": {
    "@grpc/grpc-js": "^1.13.3"
  }
}
