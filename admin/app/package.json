{
  "name": "@quintessential-sft/admin-front",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "dev:ts": "yarn dev & yarn ts:watch",
    "ts": "tsc --noEmit --incremental",
    "ts:watch": "yarn ts --watch",
    "build": "next build",
    "start": "next start -p 8080",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect",
    "build:docker": "docker build -t quintessential.azurecr.io/conduit-admin:latest -f ./Dockerfile ../../ && docker push quintessential.azurecr.io/conduit-admin:latest",
    "clean": "rm -rf public/ .next/",
    "next-lint": "next lint",
    "prettier:write": "npx prettier --write **/*.{js,ts,tsx,json} && npx prettier --write *.{js,ts,json}",
    "lint": "tsc --noEmit && eslint --ext .js,.ts,.tsx ./"
  },
  "dependencies": {
    "@date-io/moment": "1.x",
    "@material-ui/core": "^4.9.4",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.44",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "latest",
    "@reduxjs/toolkit": "^1.6.1",
    "@types/react-dropzone": "^5.1.0",
    "@types/react-window": "^1.8.5",
    "@types/react-window-infinite-loader": "^1.0.5",
    "axios": "^0.19.2",
    "clsx": "^1.1.0",
    "formik": "^2.1.4",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "memoize-one": "^6.0.0",
    "moment": "^2.24.0",
    "next": "^11.1.2",
    "next-redux-wrapper": "^5.0.0",
    "notistack": "^1.0.10",
    "prismjs": "^1.25.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-hook-form": "^7.19.1",
    "react-redux": "^7.2.0",
    "react-simple-code-editor": "^0.11.0",
    "react-spring": "^9.1.2",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.6",
    "react-window-infinite-loader": "^1.0.7",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "snyk": "^1.321.0",
    "swr": "^0.1.18",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.7",
    "@types/js-cookie": "^2.2.7",
    "@types/material-ui": "^0.21.9",
    "@types/node": "^16.7.3",
    "@types/prismjs": "^1.16.6",
    "@types/react": "^17.0.19",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/react-window-infinite-loader": "^1.0.5",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "typescript": "^4.4.2"
  },
  "snyk": true,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn prettier:write && yarn lint"
    }
  }
}
