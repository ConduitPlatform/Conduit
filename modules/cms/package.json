{
  "name": "@conduit/cms",
  "version": "1.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "build": "rimraf dist && tsc",
    "postbuild": "copyfiles -u 1 src/**/*.proto src/*.proto src/**/*.json ./dist/",
    "build:docker": "docker build -t quintessential.azurecr.io/conduit-cms:latest -f ./Dockerfile ../../ && docker push quintessential.azurecr.io/conduit-cms:latest"
  },
  "license": "ISC",
  "dependencies": {
    "@grpc/proto-loader": "^0.5.4",
    "@quintessential-sft/conduit-grpc-sdk": "^1.0.1",
    "convict": "^6.0.0",
    "deepdash": "^5.0.3",
    "grpc": "^1.24.2",
    "lodash": "^4.17.15"
  },
  "directories": {
    "lib": "src"
  },
  "files": [
    "src"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "devDependencies": {
    "@types/convict": "^4.2.1",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.150",
    "copyfiles": "^2.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.7.5"
  }
}
