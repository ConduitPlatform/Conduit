{
  "name": "@conduit/actor",
  "version": "1.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "build": "rimraf dist && tsc",
    "postbuild": "copyfiles -u 1 src/**/*.proto src/*.proto src/**/*.json src/**/**/*.json ./dist/",
    "build:docker": "docker build -t quintessential.azurecr.io/conduit-actor:latest -f ./Dockerfile ../../ && docker push quintessential.azurecr.io/conduit-actor:latest"
  },
  "license": "ISC",
  "directories": {
    "lib": "src"
  },
  "files": [
    "src"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@quintessential-sft/conduit-grpc-sdk": "^1.0.1",
    "@grpc/proto-loader": "^0.5.4",
    "axios": "^0.21.2",
    "bull": "^3.22.4",
    "convict": "^6.0.0",
    "@grpc/grpc-js": "^1.3.4",
    "lodash": "^4.17.15",
    "deepdash": "^5.0.1",
    "moment": "^2.24.0",
    "uuid": "^7.0.2",
    "vm2": "^3.9.5"
  },
  "devDependencies": {
    "@types/bull": "^3.15.1",
    "@types/convict": "^4.2.1",
    "@types/lodash": "^4.14.149",
    "@types/uuid": "^7.0.2",
    "copyfiles": "^2.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  }
}
