{
  "name": "@conduit/sms",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "postbuild": "copyfiles -u 1 src/**/*.proto src/*.proto src/**/*.json ./dist/",
    "build": "rimraf dist && tsc",
    "prepare": "npm run build",
    "build:docker": "docker build -t quintessential.azurecr.io/conduit-sms:latest -f ./Dockerfile ../../ && docker push quintessential.azurecr.io/conduit-sms:latest"
  },
  "directories": {
    "lib": "src"
  },
  "files": [
    "src"
  ],
  "license": "ISC",
  "dependencies": {
    "@quintessential-sft/conduit-grpc-sdk": "^1.0.1",
    "convict": "^6.0.0",
    "grpc": "^1.24.2",
    "lodash": "^4.17.15",
    "twilio": "3.52.0"
  },
  "devDependencies": {
    "@types/convict": "^4.2.1",
    "@types/express": "^4.16.1",
    "@types/lodash": "^4.14.149",
    "copyfiles": "^2.2.0",
    "express": "~4.16.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "express": "~4.16.1"
  }
}
